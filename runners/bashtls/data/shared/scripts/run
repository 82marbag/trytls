#!/bin/sh

stubs=$1      #.stubs file generated by init scripts   (e.g. python3-urllib)
run=$2        #what file to run inside the stub folder (e.g. run.py)
backend_conf=$3    #what backend to test to (e.g. badssl_https_conf)

while read -r stub; do  #read next stub

  echo ""
  echo ------$stub---------
  echo ""

  bash /etc/shared/simplerunner/run python3 "/etc/shared/stubs/$stub/$run" \
    "/etc/shared/simplerunner/conf/const/$backend_conf" "_" "/etc/shared/simplerunner"

done <"$stubs"

#bash /etc/shared/stubs/python3-urllib/run.py
